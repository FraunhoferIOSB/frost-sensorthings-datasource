{
  "name": "frost-sensorthings-datasource",
  "version": "0.0.6",
  "description": "A data source plugin for loading data from OGC SensorThings APIs into Grafana.",
  "keywords": [
    "SensorThings API",
    "FROST",
    "grafana",
    "OGC"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "homepage": "https://github.com/FraunhoferIOSB/frost-sensorthings-datasource",
  "bugs": "https://github.com/FraunhoferIOSB/frost-sensorthings-datasource/issues",
  "repository": "github:FraunhoferIOSB/frost-sensorthings-datasource",
  "author": {
    "name": "Fraunhofer IOSB",
    "email": "frost@iosb.fraunhofer.de",
    "url": "https://www.iosb.fraunhofer.de/ILT"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "^10.0.3",
    "@grafana/runtime": "^10.0.3",
    "@grafana/ui": "^10.0.3",
    "@material-table/core": "^5.1.9",
    "@material-ui/core": "^4.12.4",
    "dayjs": "^1.11.10",
    "jsonata": "^2.0.3",
    "jsonpath-plus": "^7.0.0",
    "memory-cache": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@emotion/core": "11.0.0",
    "@grafana/e2e": "10.0.3",
    "@grafana/e2e-selectors": "10.1.0",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.4.0",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.197",
    "@types/memory-cache": "^0.2.3",
    "@types/node": "^20.9.0",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "emotion": "10.0.27",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.3",
    "prettier": "^3.0.2",
    "react-virtualized-auto-sizer": "^1.0.20",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.66.1",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.2.2",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "yarn@"
}
